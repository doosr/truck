{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.{png,jpg,js,css}",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/api/telemetry",
            "destination": "index.js"
        },
        {
            "source": "/(.*)",
            "destination": "index.js"
        }
    ]
}